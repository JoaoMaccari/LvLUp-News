
<%- include('../../partials/header') -%>
<%- include('../../partials/homenavbar') -%>

<body>
    <div class="container mt-2">
        
        <hr>
        <% result.articles.rows.forEach(article =>{ %>
            <div class="card">
                <div class="card-header">
                   <h2><%= article.title %></h2> 
                </div>

                <div class="card-body">
                    <a class="btn btn-success" href="/<%= article.slug %>">Ler artigo</a>
                </div>
            </div>
            <br>
        <% }) %>
    </div>
    
</body>

